				<br>

        <div class="search container">
        <div class="col-lg-6 col-md-offset-3">
        <div class="input-group ">

      
        <%= form_tag rack_path, method: :get do %>
        <div class="col-md-4">
        <%= text_field_tag :item_name, params[:search], placeholder: "Searching for...", class: "form-control" %>

        </div>
        <div class="col-md-4">
        <%= text_field_tag :city, params[:search], placeholder: "At location... ", class: "form-control address" %>
        </div>
        <div class="col-md-3">
         <%= submit_tag "Search", class: "btn form-control" %>
        </div>  

        <% end %>

      </div>
      </div>
      </div>


